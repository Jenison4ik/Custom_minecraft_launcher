{
  "name": "server_mc",
  "version": "0.0.1",
  "description": "Server handler for launcher queries",
  "main": "index.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node index.ts",
    "docker:build": "docker build -t server_mc .",
    "docker:run": "docker run -d --env-file .env -p $(grep PORT .env | cut -d '=' -f2):$(grep PORT .env | cut -d '=' -f2) --name server_mc_container server_mc"
  },
  "author": "Evgenii Solovev",
  "type": "module",
  "license": "MIT",
  "dependencies": {
    "archiver": "^7.0.1",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "extract-zip": "^2.0.1",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2"
  }
}
